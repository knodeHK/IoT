CREATE TABLE thingsboard.ts_kv_latest_cf (
    entity_type text,
    entity_id timeuuid,
    key text,
    bool_v boolean,
    dbl_v double,
    long_v bigint,
    str_v text,
    ts bigint,
    PRIMARY KEY ((entity_type, entity_id), key)
) WITH CLUSTERING ORDER BY (key ASC);

